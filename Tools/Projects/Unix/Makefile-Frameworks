include	../../Library/Configuration.mk
include	../../Library/SharedBuildRules.mk
include	../../Library/SharedMakeVariables.mk

SrcDir						=	$(StroikaRoot)Tools/Sources/Stroika/Frameworks/


VPATH		=	$(SrcDir)

SubDirs=				\
	WebServer	\




all:	$(SubDirs)
	@for SUBPROJ in $(SubDirs) ; do \
		($(MAKE) --directory $$SUBPROJ --no-print-directory $@);\
    done	

clean clobber::	$(SubDirs)
	@for SUBPROJ in $(SubDirs) ; do \
		($(MAKE) --directory $$SUBPROJ --no-print-directory $@);\
    done	


