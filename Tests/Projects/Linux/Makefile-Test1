include	../Library/Configuration.mk
include	../Library/SharedBuildRules.mk

CFLAGS= -c -std=c++0x $(DEBUGFLAG) $(Includes)


_Back2Top_	=	../../../../

ObjDir                          =       ./
SrcDir                          =       $(_Back2Top_)Tests/1
