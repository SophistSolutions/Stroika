include ../../../Configuration.mk
include	../../../SharedBuildRules.mk
include	../../../SharedMakeVariables.mk

SrcDir                          =       $(StroikaRoot)Library/Sources/Stroika/Foundation/DataExchange/Compression/

vpath %cpp $(SrcDir)

Objs	=	\
	$(ObjDir)Reader.o\

SubDirs	=	\
	Zip \

.PHONY:				$(SubDirs)


all:	$(Objs) $(SubDirs)


Zip_OBJS := $(addprefix "Zip/", $(shell $(MAKE) --directory Zip -s list-objs))


list-objs::
	@echo $(Zip_OBJS)


$(SubDirs):
	@$(MAKE) --directory $@ -s
