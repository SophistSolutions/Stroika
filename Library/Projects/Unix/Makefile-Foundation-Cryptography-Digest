include ../../../Configuration.mk
include	../../../SharedMakeVariables.mk
include	../../../SharedBuildRules.mk


SrcDir						=	$(StroikaRoot)Library/Sources/Stroika/Foundation/Cryptography/Digest/

.PHONY:			Algorithm


SubDirs	=	\
	Algorithm	\

vpath %cpp $(SrcDir)

Objs	=	\
	$(ObjDir)Digester.o\
	$(ObjDir)DigestDataToString.o\
	$(ObjDir)ResultTypes.o\

all:	$(Objs)  $(SubDirs)


Algorithm_OBJS := $(addprefix "./Algorithm/", $(shell cd Algorithm; $(MAKE) -s list-objs))


list-objs::
	@echo $(Algorithm_OBJS)

Algorithm:
	(cd Algorithm; $(MAKE) -s)
