include ../../../Configuration.mk
include	../../../SharedBuildRules.mk
include	../../../SharedMakeVariables.mk

RelPathToStroikaDevRoot	=	../../../../../../
SrcDir					=	$(RelPathToStroikaDevRoot)Library/Sources/Stroika/Foundation/Characters/Concrete

SubDirs	=	\
	Private \

vpath %cpp $(SrcDir)

Objs	=	\
	$(ObjDir)String_ExternalMemoryOwnership_ApplicationLifetime.o\
	$(ObjDir)String_ExternalMemoryOwnership_StackLifetime.o\

.PHONY:	$(SubDirs)

all:	$(Objs) $(SubDirs)

PRIVATE_OBJS := $(addprefix "Private/", $(shell $(MAKE) --directory Private -s list-objs))


list-objs::
	@echo $(PRIVATE_OBJS)


Private:
	@$(MAKE) --directory Private -s
