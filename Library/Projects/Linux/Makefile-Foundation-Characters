include ../Configuration.mk

CFLAGS= -c -std=c++0x $(DEBUGFLAG)


ObjDir                          =       ./
SrcDir                          =       ../../../Sources/Stroika/Foundation/Characters
Includes                        =       -I.


VPATH = $(SrcDir)

Objs	=	\
	$(ObjDir)Character.o\
	$(ObjDir)CodePage.o\
	$(ObjDir)StringUtils.o\
	$(ObjDir)TChar.o\
	$(ObjDir)TString.o\

.cpp.o:
