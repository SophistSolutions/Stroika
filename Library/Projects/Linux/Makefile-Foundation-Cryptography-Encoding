include ../../../Configuration.mk
include	../../../SharedMakeVariables.mk
include	../../../SharedBuildRules.mk


RelPathToStroikaDevRoot		=	../../../../../../
SrcDir						=	$(RelPathToStroikaDevRoot)Library/Sources/Stroika/Foundation/Cryptography/Encoding/

.PHONY:			Algorithm


SubDirs	=	\
	Algorithm	\

vpath %cpp $(SrcDir)

Objs	=	\
	$(ObjDir)OpenSSLCryptoStream.o\


all:	$(Objs)  $(SubDirs)


Algorithm_OBJS := $(addprefix "./Algorithm/", $(shell cd Algorithm; make -s list-objs))


list-objs::
	@echo $(Algorithm_OBJS)

Algorithm:
	(cd Algorithm; make -s)
