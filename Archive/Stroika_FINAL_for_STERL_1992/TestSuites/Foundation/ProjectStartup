Execute "{Stroika_bin}ProjectStartup"

# Project_bin already properly set
Set Project_src "{Stroika_src}TestSuites:Foundation:"

Set ProgramName ""; Export ProgramName;
Set ProjectName	"TestSuite"; Export ProjectName;

AddMenu Lookup '(-' ''
AddMenu	Lookup	'{Project_bin}' 					'Lookup "{Project_bin}" > Dev:Null │ Dev:Null'
AddMenu	Lookup	'{Project_src}' 					'Lookup "{Project_src}" > Dev:Null │ Dev:Null'
AddMenu	Lookup	'{Project_src}Headers:' 			'Lookup "{Project_src}Headers" > Dev:Null │ Dev:Null'
AddMenu	Lookup	'{Project_src}Sources:' 			'Lookup "{Project_src}Sources" > Dev:Null │ Dev:Null'


AddSourceLookupMenu Find "Project" 			"{Project_src}"
AddSourceLookupMenu Find "Project-Headers"	"{Project_src}"Headers:
AddSourceLookupMenu Find "Project-Sources"	"{Project_src}"Sources:




Set	TestSuites Х
	" Х
	tArray Х
	tBag Х
	tBag_Array Х
	tBag_LinkList Х
	tDate Х
	tDeque Х
	tDeque_CircularArray Х
	tDeque_FixedCircularArray Х
	tDeque_DoubleLinkList Х
	tException Х
	tFile Х
	tFormat Х
	tHashTable Х
	tInt64 Х
	tMapping Х
	tMapping_SortedLinkList Х
	tMapping_Array Х
	tMapping_FixedArray Х
	tMapping_FixedSortedArray Х
	tMapping_SortedArray Х
	tMapping_HashTable Х
	tMapping_LinkList Х
	tMath Х
	tMemory Х
	tQueue Х
	tQueue_CircularArray Х
	tQueue_FixedCircularArray Х
	tQueue_DoubleLinkList Х
	tSequence Х
	tSequence_Array Х
	tSequence_DoubleLinkList Х
	tSet_Array Х
	tSet_BitString Х
	tStack_Array Х
	tStack_FixedArray Х
	tStack_LinkList Х
	tString Х
	tTally Х
	tTally_Array Х
	tTally_LinkList Х
	tTally_FixedArray Х
	tTime Х
	"

AddMenu	Project	'Execute Program/E'													Х
'																					Х
	Begin; 																			Х
		for p in {TestSuites};														Х
			echo Running: {p};														Х
			"{Project_bin}"{p} || (Echo "TEST FAILED!!!!" && Set Status 0);			Х
		end;																		Х
	End ии "{Worksheet}"'
